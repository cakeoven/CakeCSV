{
  "name": "cakeoven/cake_csv",
  "description": "A CakePHP Component to export CSV files",
  "homepage": "http://github.com/cakeoven/CakeCSV",
  "type": "cakephp-plugin",
  "keywords": [
    "cakeoven",
    "cakephp",
    "plugin",
    "component",
    "csv"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "George Mponos",
      "email": "gmponos@gmail.com",
      "homepage": "http://gmponos.webthink.gr",
      "role": "developer"
    }
  ],
  "support": {
    "issues": "https://github.com/cakeoven/CakeCSV/issues"
  },
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "^2.7",
    "composer/installers": "*"
  },
  "require-dev": {
    "cakephp/cakephp-codesniffer": "dev-master",
    "phpunit/phpunit": "*"
  },
  "suggest": {
    "apigen/apigen": "*"
  },
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    },
    "installer-name": "CakeCsv"
  }
}
