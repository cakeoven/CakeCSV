{
  "name": "cake/cake_csv",
  "description": "A CakePHP Component to export CSV files",
  "type": "cakephp-plugin",
  "license": "MIT",
  "keywords": [
    "cakephp",
    "plugin",
    "component",
    "csv"
  ],
  "homepage": "http://github.com/gmponos/CakeCSV",
  "support": {
    "issues": "https://github.com/gmponos/CakeCSV/issues"
  },
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "^2.6"
  },
  "require-dev": {
    "cakephp/cakephp-codesniffer": "*",
    "phpunit/phpunit": "*"
  },
  "suggest": {
    "apigen/apigen": "Produce Documentations very easily via DocBlocks"
  },
  "prefer-stable": true
}
