{
  "name": "cakeoven/cake_csv",
  "description": "A CakePHP Component to export CSV files",
  "homepage": "http://github.com/cakeoven/CakeCSV",
  "type": "cakephp-plugin",
  "keywords": [
    "cakeoven",
    "cakephp",
    "plugin",
    "component",
    "csv"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "George Mponos",
      "email": "gmponos@gmail.com",
      "homepage": "http://gmponos.webthink.gr",
      "role": "developer"
    }
  ],
  "support": {
    "issues": "https://github.com/cakeoven/CakeCSV/issues"
  },
  "autoload": {
    "psr-4": {
      "CakeCsv\\": "src",
      "CakeCsv\\Test\\": "tests"
    }
  },
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "^3.2",
    "psr/http-message": "*"
  },
  "require-dev": {
    "cakephp/cakephp-codesniffer": "~2.0",
    "phpunit/phpunit": "*"
  },
  "suggest": {
    "apigen/apigen": "*"
  },
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-3.x": "3.x-dev"
    }
  }
}
